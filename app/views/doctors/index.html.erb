<div class="row">
  <div class="col s9">
    <h4>Doctors</h4>
  </div>
  <div class="col s3">
    <a class="waves-effect waves-light btn" href="/doctors/new">New Doctor</a>
  </div>
</div>

<ul>
  <% @all_doctors.sort_by(&:first_name).each do |doctor| %>
    <li>
      <%= link_to(doctor.first_name, "/doctors/#{doctor.id}") %>  <%= link_to(doctor.last_name, "/doctors/#{doctor.id}") %>
    </li>
  <% end %>
</ul>
